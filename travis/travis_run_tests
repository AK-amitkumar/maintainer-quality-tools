#!/usr/bin/env python

from __future__ import print_function
import subprocess


def main(test_list):
    results = []
    for test in test_list:
        res = subprocess.call(test)
        results.append(res)

    print("="*20)
    for test, res in zip(test_list, results):
        print("| %s\t%s" % (' '.join(test), res))
    print("="*20)

    return(max(results))

if __name__ == '__main__':
    tests = [
        ['test_flake8'],
        # ['test_pylint'],
        ['test_server'],
    ]
    exit(main(tests))
